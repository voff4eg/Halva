include includes/head
    #map
    script.
        $(document).ready(function () {
            initMap();
        });
        function initMap() {
            var addressPoints = !{JSON.stringify(addressObject)}
            var map = new L.Map('map', {center: new L.LatLng(55.7522, 37.6156), zoom: 17, zoomAnimation: false});
            var osm = new L.TileLayer('http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png');
            var yndx = new L.Yandex();
            var googleLayer = new L.Google('ROADMAP');
            var mainMap = new L.TileLayer('http://velios.myjino.ru/maptiles/main/{z}/{x}/{y}.jpg');
            var universiadaMap = new L.TileLayer('http://velios.myjino.ru/maptiles/universiada/{z}/{x}/{y}.jpg');
            for (var i = 0; i < addressPoints.length; i++) {
                var a = addressPoints[i];
                var marker = L.marker(new L.LatLng(a.cx, a.cy), {title: a.name});
                marker.bindPopup(a.name);
                //markers.addLayer(marker);
                map.addLayer(marker);
            }
            map.addLayer(yndx);
            map.addControl(new L.Control.Layers({
                "Yandex": yndx,
                'OSM': osm,
                "Google": googleLayer,
                "Основной чертеж": mainMap,
                "Универсиада": universiadaMap
            }));
        }

include includes/footer